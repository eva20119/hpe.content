<script>
var $jq = jQuery.noConflict();
$jq(document).ready(function(){
  $jq('input').each(function(){
    if($jq(this).data('checked') == 'True'){
      $jq(this).prop('checked',true)
      $jq(this).prop('disabled',true)
    }
  })
  $jq('.oi_vc_button').click(function(){
    select_date = []
    $jq('input:checked').each(function(){
      if($jq(this).prop('disabled') != true){
        select_date.push(this.name)            
      }
    })
    debugger
    url = location.href.replace('eat_blog', 'update_activity')
    data = {
      'select_date': select_date,
      'event': '食在有道理'
    }

    $jq.ajax({
      type: "post",
      url: url,
      data: data,
      success: function (response) {
        if(response == 'success'){
           alert(`【報名資料已送出】確認收到您的報名資料
請到”我的資料”確認聯絡方式無誤，保障您收到活動通知的權益
每場次名額有限，活動採事先報名，活動開始後30分鐘若報名者未進行報到，剩餘名額將開放現場報名`)
          location.reload() 
        }
        else if(response == 'loseData'){
           alert('您的填料尚未填寫，請前後填寫完整員工資料後再報名活動，謝謝。')
           window.location = '${abs_url}/profile'
        }
      }
    });
  })
})
</script>
