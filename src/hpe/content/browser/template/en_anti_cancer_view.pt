<script>
var $ = jQuery.noConflict();
$(function(){
  $('input').each(function(){
    if($(this).data('condition') == 'True'){
      $(this).prop('checked',true)
      $(this).prop('disabled',true)
    }
  })

    $('.oi_vc_button').click(function(){
        select_date = []
        special = []
        $('input:checked').each(function(){
            if( $(this).prop('disabled') != true){
                if( this.name == '6_4'){
                    ta = $(this).data('target')
                    time = $(this).data('time')
                    special.push( [time, $('#' + ta).val()] )
                }else{
                    select_date.push($(this).data('time'))
                }
            }
        })
        url = location.href.replace('en_anti_cancer_view', 'update_activity')
        data = {
          'select_date': select_date,
          'special': special,
          'event': '防癌你我他',
          'step': 0
        }
        $.ajax({
          type: "post",
          url: url,
          data: data,
          success: function (response) {
            if(response == 'success'){
                alert(`【Your registration form has been sent successfully!】
* Your registration information has been received.
*Please check “Personal Information”, and confirm your contact info to make sure that you don't miss any notifications.`)
                location.reload()
             }
             else if(response == 'loseData'){
                alert(`Your profile is empty ，Please fill in the complete employee information before 
                    registering for the event，Thank you`)
               window.location = '${abs_url}/profile'
             }
          },
          error: function (response) {
            alert('The system did not successfully receive your registration request. Please try again later.' + 
              'If you repeat this message please contact us，\n' +
              '\nHPE Taiwan Wellness Team\n' +
              'Tel: 02-2341-8301#13\n' +
              'Email: hpewellness@accessus.biz')
          }
        });
    })
})
</script>


