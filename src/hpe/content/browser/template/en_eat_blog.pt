<script>
var $jq = jQuery.noConflict();
$jq(document).ready(function(){
  $jq('input').each(function(){
    if($jq(this).data('checked') == 'True'){
      $jq(this).prop('checked',true)
      $jq(this).prop('disabled',true)
    }
  })
  $jq('.oi_vc_button').click(function(){
    select_date = []
    $jq('input:checked').each(function(){
      if($jq(this).prop('disabled') != true){
        select_date.push(this.name)            
      }
    })
    debugger
    url = location.href.replace('en_eat_blog', 'update_activity')
    data = {
      'select_date': select_date,
      'event': '食在有道理'
    }

    $jq.ajax({
      type: "post",
      url: url,
      data: data,
      success: function (response) {
        if(response == 'success'){
           alert(`【Your registration form has been sent successfully!】
* Your registration information has been received.
*Please check “’Personal Information”, and confirm your contact info to make sure that you don't miss any notifications.
Registration is limited. Please submit your application before the deadline. If the registers are not registered after 30 minutes from the starting time, the remaining amount can be used for on-site registration.
In order to encourage our staff to work out more, everyone can register for the all 4 sessions of event. And please contact us via email or phone in advance if you will not be able to come, in order to be respectful of other registers.`)
          location.reload() 
        }
      }
    });
  })
})
</script>
