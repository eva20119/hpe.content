<script>
var jq = jQuery.noConflict(true);
jq(function(){
    var img_data = []
    jq('#image').change(function (e) {
        len_audit_data = jq('#len_audit_data').text()
        if(jq('.select1').val() == '無' || jq('.select1').val() == '北部' || jq('.select1').val() == '中部' || jq('.select1').val() == '南部'){
            jq.notify('You have not selected a location, or the location is wrong', 'error')
            jq('#image').val('')
            return false
        }
        if(len_audit_data >= 6){
            jq.notify('Review picture to full, do not upload again', 'error')
            jq('#image').val('')
            return false
        }
        if (this.files && this.files[0]) {
            var FR = new FileReader();

            FR.addEventListener("load", function(e) {
                count = 0
                jq('.upload').each(function(){
                    if(! jq(this).hasClass('full')){
                        count ++
                        jq(this).css('backgroundImage', 'url(' + e.target.result + ')' )
                        jq(this).css('background-position', 'center' )
                        jq(this).css('background-repeat', 'no-repeat')
                        jq(this).addClass('full')
                        select1 = jq('.select1').val()

                        select2 = []
                        jq("input[type=checkbox]:checked").each(function(){
                            select2.push(this.id)
                        })
                        select2 = select2.join('/')

                        data = [select1, select2, e.target.result]
                        jq(this).find('input').val(data)
                        jq(this).find('.item-serv-title').text(select1)
                        jq(this).find('.item-serv-descr').text(select2)

                        jq('.select2').val('無')
                        jq('.select1').val('無')
                        jq('#image').val('')

                        jq(this).siblings().show()
                        jq('#len_audit_data').text(parseInt(len_audit_data) + 1)

                        return false
                    }
                })
                if(count == 0){
                    jq.notify('Upload space is full', 'error')
                }
            });
            FR.readAsDataURL( this.files[0] );
        }
    });
    jq('#upload_btn').click(function(){
        jq('.upload_input').each(function(){
            if(jq(this).val()){
                data = {
                    'img_data': jq(this).val()
                }
                url = location.href.replace('en_bicycle_upload_view', 'upload_bicycle_image')
                jq.ajax({
                    type: "post",
                    url: url,
                    data: data,
                    success: function (rep) {
                        if(rep == 'error'){
                            jq.notify('Update Error', 'error')
                        }else{
                            jq.notify('Update Success', 'success')
                        }
                    }
                });   
            }
        setTimeout(function(){
            location.reload()
        }, 1500)
        })
    })
    jq('.cross').click(function(){
        jq(this).siblings().css('backgroundImage', 'url(++plone++hpe.content/img/upload.jpg)')
        jq(this).siblings().find('.item-serv-title').text('Designated spots')
        jq(this).siblings().find('.item-serv-descr').text('Item')
        jq(this).siblings().find('.upload_input').val('')
        jq(this).css('display', 'none')
        jq(this).siblings().removeClass('full')
        new_len_audit_data = jq('#len_audit_data').text() - 1
        jq('#len_audit_data').text(new_len_audit_data)
    })
    jq('#language').change(function (e) { 
        e.preventDefault();
        val = jq(this).val()
        if(val == 'en'){
            location.href = 'http://hpe.mingtak.com.tw/en_bicycle_upload_view'
        }else if(val == 'ch'){
            location.href = 'http://hpe.mingtak.com.tw/bicycle_upload_view'
        }
    });

})
