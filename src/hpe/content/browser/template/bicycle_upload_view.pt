<script>
var jq = jQuery.noConflict(true);
jq(function(){
    var img_data = []
    jq('#image').change(function (e) {
        len_audit_data = jq('#len_audit_data').text()
        if(jq('.select1').val() == '無' || jq('.select1').val() == '北部' || jq('.select1').val() == '中部' || jq('.select1').val() == '南部'){
            jq.notify('您尚未選擇地點,或地點錯誤', 'error')
            jq('#image').val('')
            return false
        }
        if(len_audit_data >= 6){
            jq.notify('審查圖片以滿,請勿再上傳', 'error')
            jq('#image').val('')
            return false
        }
        if (this.files && this.files[0]) {
            var FR = new FileReader();

            FR.addEventListener("load", function(e) {
                count = 0
                jq('.upload').each(function(){
                    if(! jq(this).hasClass('full')){
                        count ++
                        jq(this).css('backgroundImage', 'url(' + e.target.result + ')' )
                        jq(this).css('background-position', 'center' )
                        jq(this).css('background-repeat', 'no-repeat')
                        jq(this).addClass('full')
                        select1 = jq('.select1').val()
                        if(jq('.select2').val() == '無'){
                            select2 = ''
                        }else{
                            select2 = jq('.select2').val()
                        }

                        data = [select1, select2, e.target.result]
                        jq(this).find('input').val(data)
                        jq(this).find('.item-serv-title').text(select1)
                        jq(this).find('.item-serv-descr').text(select2)

                        jq('.select2').val('無')
                        jq('.select1').val('無')
                        jq('#image').val('')
                        
                        jq('#len_audit_data').text(parseInt(len_audit_data) + 1)
                        return false
                    }
                })
                if(count == 0){
                    jq.notify('上傳空間以滿(6張)', 'error')
                }
            });
            FR.readAsDataURL( this.files[0] );
        }
    });
    jq('#upload_btn').click(function(){
        jq('.upload_input').each(function(){
            if(jq(this).val()){
                data = {
                    'img_data': jq(this).val()
                }
                url = location.href.replace('bicycle_upload_view', 'upload_bicycle_image')
                jq.ajax({
                    type: "post",
                    url: url,
                    data: data,
                    success: function (rep) {
                        if(rep == 'error'){
                            jq.notify('上傳失敗', 'error')
                        }else{
                            jq.notify('上傳成功', 'success')
                        }
                    }
                });   
            }
        setTimeout(function(){
            location.reload()
        }, 1500)
        })
    })
})
</script>

