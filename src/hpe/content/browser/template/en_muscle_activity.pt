<script>
var $jq = jQuery.noConflict();
$jq(document).ready(function(){
  $jq('input').each(function(){
    if($jq(this).data('checked') == 'True'){
      $jq(this).prop('checked',true)
      $jq(this).prop('disabled',true)
    }
  })
  $jq('.oi_vc_button').click(function(){
    select_date = []
    $jq('input:checked').each(function(){
      if($jq(this).prop('disabled') != true){
        select_date.push(this.name)            
      }
    })
    url = location.href.replace('en_muscle_activity', 'update_activity')
    data = {
      'select_date': select_date,
      'event': '肌力動次動',
      'step': 477
    }

    $jq.ajax({
      type: "post",
      url: url,
      data: data,
      success: function (response) {
        if(response == 'success'){
           alert(`【Your registration form has been sent successfully!】
* Your registration information has been received.
*Please check “’Personal Information”, and confirm your contact info to make sure that you don't miss any notifications.
Registration is limited. Please submit your application before the deadline. If the registers are not registered after 10 minutes from the starting time, the remaining amount can be used for on-site registration.
`)
          location.reload() 
        }
        else if(response == 'loseData'){
           alert('您的資料料尚未填寫，請前往填寫完整員工資料後再報名活動，謝謝。')
           window.location = '${abs_url}/profile'
        }
      }
    });
  })
})
</script>
